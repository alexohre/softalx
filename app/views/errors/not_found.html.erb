    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <div class="axil-error text-center">
                    <div class="inner">
                        <h1 class="title mb--20">Page not Found</h1>
                        <p class="subtitle-2 mb--30">Lorem ipsum dolor sit amet consectetur, adipisicing
                            elit. Enim, recusandae <br /> consectetur nesciunt magnam facilis aliquid amet
                            earum alias?</p>
                            <a class="axil-button btn-large btn-transparent" href="/"><span
                                class="button-text">Go Back Home</span><span class="button-icon"></span></a>
                                <%= image_tag("others/404.svg", alt: "404 image", class: "text-image") %>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="shape-group">
        <div class="shape shape-01">
            <%= image_tag("shape/404-01.svg", alt: "shape image") %>
        </div>
        <div class="shape shape-02">
            <%= image_tag("shape/404-01.svg", alt: "shape image") %>
        </div>
        <div class="shape shape-03">
            <%= image_tag("shape/404-02.svg", alt: "shape image") %>
        </div>
        <div class="shape shape-04">
            <%= image_tag("shape/404-03.svg", alt: "shape image") %>
        </div>
    </div>
</div>
